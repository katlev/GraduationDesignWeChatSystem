.flex{
    display: flex;
}
.flexPosition{
    flex:1;
}
.formHead{
    padding: 20rpx;
    border-bottom: 1rpx solid #e9eaec ;
}
.titlefont{
    color:#1c2438;
    font-size: 32rpx;
}
.contentFont{
    color: #495060;
    font-size: 30rpx;
}
.fileColor{
    color:#2d8cf0;
}
.formContent{
    padding: 0 2rpx;
    margin-bottom: 20rpx;
}
.contentItem{
    padding: 18rpx 32rpx;
    font-size: 28rpx;
    align-items: center;
    justify-content: center;
}

.confirm-btn{
    width: 630rpx!important;
    height: 76rpx;
    line-height: 76rpx;
    border-radius: 50rpx;
    margin: 40rpx 0;
    background: #07C160;
    color: #fff;
    font-size: 32rpx;
    padding: 0;
}
.confirm-btn:active{
    background-color: #07B160;
}
